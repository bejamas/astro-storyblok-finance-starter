---
import Button from '../components/Button.astro'
import { getHref } from '../utils/getHref'
const { blok, classes } = Astro.props

const label = blok.label || 'Become a Member'
const variant = blok.style || 'primary'
---

<Button label={label} href={getHref(blok.url)} variant={variant} classes={classes} />
